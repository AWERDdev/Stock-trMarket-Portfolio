{
  "version": 2,
  "routes": [
    {
      "src": "/api/auth/signup",
      "dest": "index.js"
    },
    {
      "src": "/api/auth/login",
      "dest": "index.js"
    },
    {
      "src": "/Stock",
      "dest": "index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["data/**/*"]
      }
    }
  ],
  "env": {
    "PORT": "3500"
  }
}
