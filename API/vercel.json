{
  "version": 2,
  "routes": [
    {
      "src": "/signup(?:/)?",
      "methods": ["POST", "OPTIONS"],
      "dest": "index.js"
    },
    {
      "src": "/login(?:/)?",
      "methods": ["POST", "OPTIONS"],
      "dest": "index.js"
    },
    {
      "src": "/Stock(?:/)?",
      "methods": ["GET", "OPTIONS"],
      "dest": "index.js"
    },
    {
      "src": "/isAUTH(?:/)?",
      "methods": ["GET", "OPTIONS"],
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["data/**/*"]
      }
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  }
}
